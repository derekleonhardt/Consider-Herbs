    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openid-connect": "^0.6.0",
    "mkcert": "^1.2.0",
    "mocha": "^6.2.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "react-big-calendar": "^0.24.1",
    "request": "^2.88.2",
    "should": "^13.2.3",
